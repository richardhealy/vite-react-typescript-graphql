    "clsx": "^1.2.1",
    "cross-fetch": "^3.1.6",
    "flowbite": "^1.6.5",
    "graphql": "^16.6.0",
    "jsdom": "^22.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/client-preset": "3.0.1",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/schema-ast": "^3.0.1",
