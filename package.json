    "@vitejs/plugin-react": "^4.0.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-c8": "^0.31.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
